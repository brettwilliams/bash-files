alias beep="for i in a b c d e f; do sleep .1 && echo -n ""; done"
alias disku="du -hsk * | sort -n | awk '{ print \$2; }' | xargs du -hcs"
alias ll="ls -l"
alias ls='command ls --color -bCF'
alias ll='command ls -bl --color'
alias d='dirs -l -v'
alias xpg='pager -A 10 -N --xmpp -s'
alias epg='pager -A 10 -N --email -s'
alias gmake='gmake -s'
